{
  "name": "chatbot-extension",
  "version": "1.0.0",
  "description": "An evaluation tool to check the accessibility of the current chatbot interface.",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build-prod": "cross-env NODE_ENV=production webpack --hide-modules --config webpack.config-production.js",
    "build-dev": "cross-env NODE_ENV=development webpack  --config webpack.config-development.js",
    "build-debug": "cross-env NODE_ENV=debug node --trace-deprecation ./node_modules/webpack/bin/webpack.js --config webpack.config-debug.js",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules ",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch",
    "dev": "NODE_ENV=development webpack --watch"
  },
  "dependencies": {
    "@qualweb/act-rules": "^0.6.37",
    "@qualweb/best-practices": "^0.6.19",
    "@qualweb/locale": "0.1.18",
    "@qualweb/qw-page": "0.2.19",
    "@qualweb/util": "0.5.37",
    "@qualweb/wcag-techniques": "0.3.17",
    "file-saver": "^2.0.2",
    "highlight.js": "^9.18.1",
    "vue": "^3.5.11",
    "vue-code-highlight": "^0.7.4",
    "vue-highlight.js": "^3.1.0",
    "vue-router": "^4.4.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "@types/chrome": "^0.0.74",
    "@vue/compiler-sfc": "^3.5.11",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^7.1.2",
    "deepmerge": "^4.2.2",
    "ejs": "^2.6.1",
    "eslint": "^9.12.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^2.9.1",
    "node-sass": "^7.0.0",
    "sass": "^1.26.2",
    "sass-loader": "^16.0.2",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^4.0.0",
    "vue-loader": "^17.3.1",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-ext-reloader": "^1.1.13"
  }
}
